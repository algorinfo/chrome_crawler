{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://algorinfo.com/chrome_response_v1.json",
    "type": "object",
    "title": "A Chrome response",
    "description": "Crawl a web page using Puppeteer",
    "default": {},
    "examples": [
        {
            "html": "<html></html>",
            "version": 1
        }
    ],
    "required": [
        "html",
        "version"
    ],
    "properties": {
        "html": {
            "$id": "#/properties/html",
            "type": "string",
            "title": "HTML content of a webpage",
            "description": "It has the full html content of a webpage",
            "default": "",
            "examples": [
                "<html></html>"
            ]
        },
        "version": {
            "$id": "#/properties/version",
            "type": "integer",
            "title": "The version schema",
            "description": "It declares to which schema version belongs",
            "default": 0,
            "examples": [
                1
            ]
        }
    },
    "additionalProperties": true
}
